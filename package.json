   "scripts": {
      "test": "mocha -r ts-node/register src/tests/*.ts",
      "test:coverage": "tsc && nyc --reporter=text mocha -r ts-node/register src/tests/*.ts",
      "tsc": "tsc",
      "start": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/index.ts",
      "prod": "tsc && node ./build/app.js",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "lint": "tsc --noEmit && eslint src/**/*.{js,ts,tsx} --ignore-path .eslintignore",
      "fix": "tsc --noEmit && eslint src/**/*.{js,ts,tsx} --ignore-path .eslintignore --fix"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run lint",
         "pre-push": "npm run test"
      }
   }
